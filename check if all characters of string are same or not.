class Solution
{
    public:
        bool check (string s)
        {
        	char c=s[0];
        	int cnt=0;
        	int n=s.size();
        	for(int i=1;i<s.size();i++){
        	    if(c==s[i]){
        	        cnt++;
        	    }
        	}
        	if(cnt==n-1){
        	    return 1;
        	}
        	return 0;
        }
};
